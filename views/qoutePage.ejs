<body>
    <section id="web-type">

        <div class="sect-header">
            <h1>Choose your desired Web Type</h1>
        </div>

        <div class="sect-parent">

            <div class="sect-child" data-name="Standard Website" data-price="1200.00">
                <div class="radio-button"></div>
                <h3>Standard Website</h3>
            </div>

            <div class="sect-child" data-name="E-commerce Website" data-price="4500.00">
